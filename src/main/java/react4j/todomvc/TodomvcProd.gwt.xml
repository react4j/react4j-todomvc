<module rename-to='todomvc'>
  <inherits name='react4j.todomvc.Todomvc'/>
  <inherits name='com.google.gwt.precompress.Precompress'/>

  <set-property name='jre.checks.checkLevel' value='MINIMAL'/>
  <set-property name='compiler.stackMode' value='strip'/>
  <set-property name='arez.enable_observer_error_handlers' value='false'/>
  <set-property name='arez.purge_reactions_when_runaway_detected' value='false'/>

  <add-linker name='sso'/>

  <source path='ignore'/>
</module>
